alias lock="xscreensaver-command -lock"
alias ls="\ls -F --color=auto"
alias ll='ls -l --color=auto'
alias cd..='cd ..'
alias lt='\ls -altr'
alias l='ll' #
alias lh='ll --si -r' # For displaying Nice labels
alias c='clear'
